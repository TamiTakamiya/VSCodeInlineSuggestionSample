{
	"name": "vscode-inline-suggestion-sample",
	"displayName": "VSCode Inline Suggestion Sample",
	"description": "How to use inline suggesion in VS Code",
	"version": "0.0.1",
	"publisher": "Tami Takamiya",
	"private": true,
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/TamiTakamiya/vscode-inline-suggestion-sample"
	},
	"engines": {
